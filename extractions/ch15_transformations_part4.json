        "point_at_x_2": "When x = 2, y = 2^(-2) - 3 = 1/4 - 3 = -2.75",
        "point_at_x_neg2": "When x = -2, y = 2^2 - 3 = 1",
        "domain": "{x | x ∈ ℝ}",
        "range": "{y | y > -3}"
