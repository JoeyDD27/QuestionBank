    {
      "section_id": "3D_combined",
      "title": "Combined Operations",
      "concepts": [
        {
          "topic": "Order of Operations",
          "content": "The order of operations rules also apply to negative numbers.",
          "rules": [
            "Brackets are evaluated first.",
            "Exponents are calculated next.",
            "Divisions and Multiplications are done next, in the order that they appear.",
            "Addition and Subtractions are then done, in the order that they appear."
          ]
        }
      ],
      "examples": [
        {
          "example_number": "11",
          "question": "Find:",
          "parts": [
            {
              "part": "a",
              "expression": "3 - (-6) x 2",
              "solution": "3 - (-6) x 2 = 3 - (-12) = 3 + 12 = 15",
              "note": "multiplication first"
            },
            {
              "part": "b",
              "expression": "13 + 25 / (-5)",
              "solution": "13 + 25 / (-5) = 13 + (-5) = 13 - 5 = 8",
              "note": "division first"
            }
          ]
        }
      ]
    },
