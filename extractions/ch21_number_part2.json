      "section_id": "3D_multiplying_dividing",
      "title": "Multiplying and Dividing Directed Numbers",
      "concepts": [
        {
          "topic": "Rules for Multiplication",
          "rules": [
            "(positive) x (positive) = (positive)",
            "(positive) x (negative) = (negative)",
            "(negative) x (positive) = (negative)",
            "(negative) x (negative) = (positive)"
          ]
        },
        {
          "topic": "Rules for Division",
          "rules": [
            "(positive) / (positive) = (positive)",
            "(positive) / (negative) = (negative)",
            "(negative) / (positive) = (negative)",
            "(negative) / (negative) = (positive)"
          ]
        },
        {
          "topic": "Sign Rules Summary (Addition)",
          "rules": [
            "+ (positive) gives a (positive)",
            "- (positive) gives a (negative)",
            "+ (negative) gives a (negative)",
            "- (negative) gives a (positive)"
          ]
        }
      ],
      "examples": [
        {
          "example_number": "9",
          "question": "Find:",
          "parts": [
            {"part": "a", "expression": "3 x 7", "solution": "3 x 7 = 21"},
            {"part": "b", "expression": "3 x (-7)", "solution": "3 x (-7) = -21"},
            {"part": "c", "expression": "-3 x 7", "solution": "-3 x 7 = -21"},
            {"part": "d", "expression": "-3 x (-7)", "solution": "-3 x (-7) = 21"}
          ]
        },
