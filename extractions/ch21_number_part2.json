    {
      "section_id": "3D_adding_subtracting",
      "title": "Adding and Subtracting Directed Numbers",
      "concepts": [
        {
          "topic": "Patterns",
          "content": "Consider these patterns:",
          "addition_pattern": [
            "5 + 2 = 7",
            "5 + 1 = 6",
            "5 + 0 = 5",
            "5 + (-1) = 4",
            "5 + (-2) = 3",
            "5 + (-3) = 2"
          ],
          "subtraction_pattern": [
            "5 - 2 = 3",
            "5 - 1 = 4",
            "5 - 0 = 5",
            "5 - (-1) = 6",
            "5 - (-2) = 7",
            "5 - (-3) = 8"
          ]
        },
        {
          "rule": "Adding a negative number is the same as subtracting a positive number."
        },
        {
          "rule": "Subtracting a negative number is the same as adding a positive number."
        }
      ],
      "examples": [
        {
          "example_number": "8",
          "question": "Find:",
          "parts": [
            {
              "part": "a",
              "expression": "6 + (-3)",
              "solution": "6 + (-3) = 6 - 3 = 3"
            },
            {
              "part": "b",
              "expression": "6 - (-3)",
              "solution": "6 - (-3) = 6 + 3 = 9"
            },
            {
              "part": "c",
              "expression": "-6 + (-3)",
              "solution": "-6 + (-3) = -6 - 3 = -9"
            },
            {
              "part": "d",
              "expression": "-6 - (-3)",
              "solution": "-6 - (-3) = -6 + 3 = -3"
            }
          ]
        }
      ]
    },
