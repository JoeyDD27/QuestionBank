          "answer_latex": "\\frac{x+7}{6}",
          "solution_steps": [
            "\\frac{x+1}{2} - \\frac{x-2}{3}",
            "= \\frac{3}{3}\\left(\\frac{x+1}{2}\\right) - \\frac{2}{2}\\left(\\frac{x-2}{3}\\right)",
            "= \\frac{3(x+1)}{6} - \\frac{2(x-2)}{6}",
            "= \\frac{3(x+1) - 2(x-2)}{6}",
            "= \\frac{3x + 3 - 2x + 4}{6}",
            "= \\frac{x + 7}{6}"
          ]
        }
