          "solution_steps": [
            "\\frac{x}{2} + \\frac{3x}{4} \\quad \\{\\text{LCD} = 4\\}",
            "= \\frac{x \\times 2}{2 \\times 2} + \\frac{3x}{4}",
            "= \\frac{2x}{4} + \\frac{3x}{4}",
            "= \\frac{2x + 3x}{4}",
            "= \\frac{5x}{4}"
          ]
        },
        {
          "label": "b",
          "problem_latex": "\\frac{a}{3} - \\frac{2a}{5}",
          "answer_latex": "-\\frac{a}{15}",
          "solution_steps": [
            "\\frac{a}{3} - \\frac{2a}{5} \\quad \\{\\text{LCD} = 15\\}",
            "= \\frac{a \\times 5}{3 \\times 5} - \\frac{2a \\times 3}{5 \\times 3}",
            "= \\frac{5a}{15} - \\frac{6a}{15}",
            "= \\frac{-a}{15} \\text{ or } -\\frac{a}{15}"
          ]
        }
