        {
          "title": "Modulus Equations",
          "content": [
            {
              "type": "definition",
              "concept": "Modulus Equation",
              "description": "A modulus equation is an equation which involves the absolute value function."
            },
            {
              "type": "properties",
              "title": "Properties of Absolute Value",
              "list": [
                "|x| >= 0 for all x",
                "|-x| = |x| for all x",
                "|x|^2 = x^2 for all x",
                "|xy| = |x||y| for all x and y",
                "|x/y| = |x|/|y| for all x and y, y != 0",
                "|x - y| = |y - x| for all x and y"
              ]
            },
            {
              "type": "theorem",
              "statement": "If |x| = a where a > 0, then x = +/-a",
              "proof": [
                "If |x| = a, a > 0",
                "then |x|^2 = a^2",
                "therefore x^2 = a^2 {since |x|^2 = x^2}",
                "therefore x = +/-sqrt(a^2)",
                "therefore x = +/-a"
              ]
            },
            {
              "type": "example",
              "number": 6,
              "problem": "Solve for x:",
              "parts": [
                {
                  "part": "a",
                  "equation": "|2x + 3| = 7",
                  "solution": {
                    "steps": [
                      "2x + 3 = +/-7",
                      "2x + 3 = 7 or 2x + 3 = -7",
                      "2x = 4 or 2x = -10",
                      "x = 2 or x = -5"
                    ],
                    "answer": "x = 2 or -5"
                  }
                },
                {
                  "part": "b",
                  "equation": "|3 - 2x| = -1",
                  "solution": {
                    "explanation": "has no solution as LHS is never negative"
                  }
                }
              ]
            },
            {
              "type": "example",
              "number": 7,
              "problem": "Solve for x:",
              "parts": [
                {
                  "part": "a",
                  "equation": "|(3x + 2)/(1 - x)| = 4",
                  "solution": {
                    "steps": [
                      "(3x + 2)/(1 - x) = +/-4",
                      "(3x + 2)/(1 - x) = 4 or (3x + 2)/(1 - x) = -4",
                      "3x + 2 = 4(1 - x) or 3x + 2 = -4(1 - x)",
                      "3x + 2 = 4 - 4x or 3x + 2 = -4 + 4x",
                      "7x = 2 or 6 = x",
                      "x = 2/7 or x = 6"
                    ]
                  }
                },
                {
                  "part": "b",
                  "equation": "|x + 1| = |2x - 3|",
                  "solution": {
                    "steps": [
                      "x + 1 = +/-(2x - 3)",
                      "x + 1 = 2x - 3 or x + 1 = -(2x - 3)",
                      "4 = x or x + 1 = -2x + 3",
                      "x = 4 or 3x = 2",
                      "x = 4 or x = 2/3"
                    ],
                    "answer": "x = 2/3 or 4"
                  }
                }
              ]
            },
            {
              "type": "example",
              "number": 8,
              "problem": "Solve graphically: |x + 1| = x/2 + 2",
              "solution": {
                "method": "Graph y = |x + 1| and y = x/2 + 2 on the same set of axes.",
                "intersection_points": [(-2, 1), (2, 3)],
                "answer": "x = -2 or 2"
              }
            }
          ]
        },
        {
          "title": "Modulus Inequalities",
          "content": [
            {
              "type": "example",
              "number": 9,
              "problem": "Solve for x:",
              "parts": [
                {
                  "part": "a",
                  "inequality": "|2x - 1| < 3",
                  "solution": {
                    "steps": [
                      "-3 < 2x - 1 < 3",
                      "-2 < 2x < 4",
                      "-1 < x < 2"
                    ]
                  }
                },
                {
                  "part": "b",
                  "inequality": "|4 - x| >= 5",
                  "solution": {
                    "steps": [
                      "4 - x <= -5 or 4 - x >= 5",
                      "x >= 9 or x <= -1"
                    ]
                  }
                }
              ]
            },
            {
              "type": "example",
              "number": 10,
              "problem": "Solve for x:",
              "parts": [
                {
                  "part": "a",
                  "inequality": "|3x + 1| >= |2x - 3|",
                  "solution": {
                    "method": "Squaring both sides",
                    "steps": [
                      "|3x + 1|^2 >= |2x - 3|^2 {squaring both sides}",
                      "(3x + 1)^2 >= (2x - 3)^2 {|a|^2 = a^2}",
                      "9x^2 + 6x + 1 >= 4x^2 - 12x + 9",
                      "5x^2 + 18x - 8 >= 0",
                      "(5x - 2)(x + 4) >= 0",
                      "x <= -4 or x >= 2/5"
                    ],
                    "note": "You can only multiply or divide by a term if you are sure of its sign."
                  }
                },
                {
                  "part": "b",
                  "inequality": "|x/(x + 1)| >= 1/2",
                  "solution": {
                    "steps": [
                      "|x/(x + 1)|^2 >= (1/2)^2 {squaring both sides}",
                      "(x/(x + 1))^2 >= 1/4 {|a|^2 = a^2}",
                      "x^2/(x + 1)^2 >= 1/4",
                      "4x^2 >= (x + 1)^2 provided x != -1 {since (x + 1)^2 >= 0}",
                      "4x^2 >= x^2 + 2x + 1 provided x != -1",
                      "3x^2 - 2x - 1 >= 0 provided x != -1",
                      "(3x + 1)(x - 1) >= 0 provided x != -1",
                      "x <= -1/3, x != -1 or x >= 1"
                    ]
                  }
                }
              ]
            },
