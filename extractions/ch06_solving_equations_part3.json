      "content": {
        "topic": "Power Equations - Solutions",
        "rules": [
          {
            "condition": "If $x^n = k$ where $n$ is even, then:",
            "cases": [
              "$x = \\pm\\sqrt[n]{k}$ if $k > 0$",
              "$x = 0$ if $k = 0$",
              "there are no real solutions if $k < 0$"
            ]
          },
          {
            "condition": "If $x^n = k$ where $n$ is odd, then $x = \\sqrt[n]{k}$"
          }
        ],
        "note": "$\\sqrt[n]{k}$ is the $n$th root of $k$."
      }
