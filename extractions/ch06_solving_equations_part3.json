      "type": "worked_example",
      "content": {
        "problems": [
          {
            "label": "a",
            "equation": "$3x - 4 = 2x + 6$"
          },
          {
            "label": "b",
            "equation": "$4 - 3(2 + x) = x$"
          }
        ],
        "solutions": [
          {
            "label": "a",
            "steps": [
              "$3x - 4 = 2x + 6$",
              "$3x - 4 - 2x = 2x + 6 - 2x$ \\quad \\{\\text{subtracting } 2x \\text{ from both sides}\\}",
              "$x - 4 = 6$",
              "$x - 4 + 4 = 6 + 4$ \\quad \\{\\text{adding 4 to both sides}\\}",
              "$x = 10$"
            ],
            "check": "$\\text{LHS} = 3 \\times 10 - 4 = 26$, $\\text{RHS} = 2 \\times 10 + 6 = 26$ \\checkmark"
          },
          {
            "label": "b",
            "steps": [
              "$4 - 3(2 + x) = x$",
              "$4 - 6 - 3x = x$ \\quad \\{\\text{expanding}\\}",
              "$-2 - 3x = x$",
              "$-2 - 3x + 3x = x + 3x$ \\quad \\{\\text{adding } 3x \\text{ to both sides}\\}",
              "$-2 = 4x$",
              "$\\frac{-2}{4} = \\frac{4x}{4}$ \\quad \\{\\text{dividing both sides by 4}\\}",
              "$-\\frac{1}{2} = x$",
              "$x = -\\frac{1}{2}$"
            ],
            "check": "$\\text{LHS} = 4 - 3(2 + (-\\frac{1}{2})) = 4 - 3 \\times \\frac{3}{2} = 4 - 4\\frac{1}{2} = -\\frac{1}{2} = \\text{RHS}$ \\checkmark"
          }
        ]
      }
    },
